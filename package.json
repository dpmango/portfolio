{
  "name": "react_vite",
  "homepage": "https://github.com/dpmango/example/START/React",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run json-server\"",
    "json-server": "json-server --watch ./server/api-db.json --routes ./server/api-routes.json --port 8000",
    "build": "vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint:style": "stylelint --fix \"**/*.{css,scss,sass,html}\" --ignore-path .gitignore",
    "lint": "yarn lint:format && yarn lint:fix && yarn lint:style"
  },
  "pre-commit": "lint",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "js-cookie": "^3.0.1",
    "json-server": "^0.17.1",
    "ofetch": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.1.4",
    "react-redux": "^8.0.5",
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0",
